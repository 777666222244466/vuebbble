{
  "name": "vuebbble",
  "version": "2.3.2",
  "description": "Vue component displaying user shots using the dribbble api",
  "author": "Robbin Johansson <robbinfellow@gmail.com>",
  "main": "dist/vuebbble.min.js",
  "homepage": "https://github.com/robbinfellow/vuebbble#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/robbinfellow/vuebbble.git"
  },
  "bugs": {
    "url": "https://github.com/robbinfellow/vuebbble/issues"
  },
  "keywords": [
    "vue",
    "vuejs",
    "dribbble",
    "api",
    "shots"
  ],
  "scripts": {
    "lint": "./node_modules/.bin/eslint --ext .js,.vue src",
    "build": "rm -rf dist && ./node_modules/.bin/webpack --hide-modules -p --progress"
  },
  "dependencies": {
    "vue": "^2.5.13"
  },
  "devDependencies": {
    "axios": "^0.17.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-lodash": "^3.3.2",
    "babel-preset-env": "^1.6.1",
    "clean-webpack-plugin": "^0.1.16",
    "css-loader": "^0.28.8",
    "eslint": "^4.15.0",
    "eslint-plugin-vue": "^4.2.0",
    "vue-loader": "^13.7.0",
    "vue-template-compiler": "^2.5.13",
    "webpack": "^3.10.0"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "uglify": true,
          "modules": false,
          "targets": {
            "node": 6,
            "browsers": [
              "> 1%",
              "last 2 versions",
              "not ie <= 8"
            ]
          }
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:vue/essential"
    ],
    "rules": {
      "vue/no-unused-vars": 0
    }
  }
}
