{
  "name": "vuebbble",
  "version": "2.3.5",
  "description": "Vue component to easily display dribbble shots from a user",
  "author": "Robbin Johansson <robbinfellow@gmail.com>",
  "main": "dist/vuebbble.js",
  "homepage": "https://github.com/robbinfellow/vuebbble#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/robbinfellow/vuebbble.git"
  },
  "bugs": {
    "url": "https://github.com/robbinfellow/vuebbble/issues"
  },
  "keywords": [
    "vue",
    "vuejs",
    "dribbble",
    "api",
    "shots"
  ],
  "scripts": {
    "lint": "./node_modules/.bin/eslint --ext .js,.vue src",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules"
  },
  "dependencies": {
    "vue": "^2.5.13"
  },
  "devDependencies": {
    "axios": "^0.17.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-3": "^6.24.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "eslint": "^4.15.0",
    "eslint-plugin-vue": "^4.2.0",
    "vue-loader": "^13.0.5",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "uglify": true,
          "modules": false,
          "targets": {
            "node": 6,
            "browsers": [
              "> 1%",
              "last 2 versions",
              "not ie <= 8"
            ]
          }
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:vue/essential"
    ],
    "rules": {
      "vue/no-unused-vars": 0
    }
  }
}
